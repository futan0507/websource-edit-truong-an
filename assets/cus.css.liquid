.noheight{
	max-height:initial !important;
}
.menu-item:hover .dropdown{
	    opacity: 1;
    visibility: visible;
}
.banner.hero-banner h1 {
    margin-bottom: 40px;
    color: #fff;
}
.slider.slider-feature .btn.btn-slider.js-feature-next {
   
	background-image: url({{'icn_arrow_left_white.svg'| asset_url}});
}
.slider.slider-feature .btn.btn-slider.js-feature-prev {
	background-image: url({{'icn_arrow_left_black.svg'| asset_url}});
}
.slider.slider-activities .btn.btn-slider.js-activities-prev {
	background-image: url({{'icn_arr_black.png'| asset_url}});
   
}
.slider.slider-activities .btn.btn-slider.js-activities-next {
	background-image: url({{'icn_arr_black_right.png'| asset_url}});
}
.icn.icn-arrow-green {
    width: 5px;
    height: 15px;
	background-image: url({{'icn_arrow_left_green.svg'| asset_url}});
}
.panel-services .icon img:nth-child(2){
	display:none
}
.panel-services .icon img:nth-child(2){
	display:none
}
.panel.panel-services .item:hover .icon img:nth-child(2){
	display:block
}
.panel.panel-services .item:hover .icon img:nth-child(1){
	display:none
}
.panel-services .icon img {
   
    max-width: 32px;
}
footer .bottom .back-to-top {
	background-image: url({{'icn_back.png'| asset_url}});
}
header {
 position: relative;
}
.icn.icn-paging {
    width: 7.41px;
    height: 12px;
	background-image: url({{'icn_paging.svg'| asset_url}});
}
.paging .page-numbers:hover .icn.icn-paging {
    background-image: url({{'icn_paging_white.svg'| asset_url}});
}
.panel.panel-rich-text .group-highlight .read-more .icn {
   
	background-image: url({{'icn-arr-black.png'| asset_url}});
}

.panel.panel-rich-text .group-time .slider-for .btn.btn-slider.js-slider-for-prev {
	background-image: url({{'icn_arr_black.png'| asset_url}});
    margin-right: 6px;
}

.panel.panel-rich-text .group-time .slider-for .btn.btn-slider.js-slider-for-next {
	background-image: url({{'icn_arr_black_right.png'| asset_url}});
}
.btn-slider{
	    outline: none !important;
    box-shadow: none  !important;
}
.slider.slider-activities .item {
    width: initial !important;
    height: initial !important;
	    padding-left: 0;
	    padding: 5px;
}
.option .custom-dropdown .custom-dropdown__select{
	color:#000
}
.flexboxaction{
	display:flex;
}
.flexboxaction > div{
	margin-right:20px;
}
.flexboxaction > div a{
 display: inline-block;
    background: #296856;
    padding: 10px 34px;
    color: #fff;
}
header .h-form-search {
    position: initial;
    opacity: 1;
    visibility: visible;
	display:none;
}
 .img.kingm{
	 max-height:125px;
	 overflow:hidden
}
.panel-resources h2{
	color:#fff;
}
header .logo {
  
	background-image: none;
}
.logox img{
	max-width:150px;
}
.hamburger-menu .bar,.hamburger-menu .bar:before,.hamburger-menu .bar:after {
    background: #00B380;
}
#siteNav-menu a{
	text-align:left;
}
.slider.slider-feature .btn.btn-slider.js-feature-prev {
    left: 3px;
	background-image: url({{'icn_arrow_left_white.svg'| asset_url}});
}
#contact_email2{
	height:45px;
	padding:5px;
}
.contact-form{
	position:relative;
	margin-bottom:10px
	}
#contact_email2 + .btn-reg-popup-contact{
	    position: absolute;
    top: 0;
    right: 0;
    z-index: 999;
    height: 45px;
    width: 79px;
    padding: 0;
    background: transparent;
    border: none;
	    color: #000;
	}
#contact_email2 + .btn-reg-popup-contact:before{
	display:none;
}
#contact_email2 + .btn-reg-popup-contact i{
	
font-size: 23px;
    float: right;
    margin-right: 10px;
}

header .languages ul {
    display: flex;
}

@media (max-width:767px){
	header .languages ul li a {
    
    font-size: 11px;
}
	.slider.slider-feature h2{
		color:#fff;
	}
	.flexboxaction {
    display: flex;
    flex-wrap: wrap;
}
	.flexboxaction > div{
		width:100%;
		margin-bottom:15px;
	}
	.flexboxaction > div a {
    display: block;
    background: #296856;
    padding: 10px 34px;
    color: #fff;
    text-align: center;
    text-transform: uppercase;
}
}
.panel-resources .item:nth-child(2)  h2 {
    color: #173330;
}

.panel-resources .item:nth-child(2) .btn.btn-white {
    color: #fff;
	    border: 1px solid {{settings.btn_bg_shop}};
    background-color: transparent;
    background-image: -webkit-linear-gradient(179deg,{{settings.btn_bg_shop}} 0%,#083333 100%);
    background-image: -o-linear-gradient(179deg,{{settings.btn_bg_shop}} 0%,#083333 100%);
    background-image: linear-gradient(269deg,{{settings.btn_bg_shop}} 0%,#083333 100%);
}
.panel-resources .item:nth-child(2) .btn.btn-white:hover {
      color: {{settings.btn_bg_shop}};
    background-image: none;
    background-color: #fff;
}
.btn.btn-white:hover {
    color: #fff;
    background-color: {{settings.btn_bg_shop}} !important;
    background-image: none !important;
   
}
.panel.panel-categories .item:first-child .btn.btn-link,.panel-resources .item:nth-child(2) .btn.btn-white,
.btn.btn-green{
	background-color: {{settings.btn_bg_shop}} !important;
    background-image: none !important;
}
.slider.slider-feature .i-right .overlay,
.panel.panel-resources .item:first-child .overlay{
    background-image: none;
}
.panel-resources .item:nth-child(2) .btn.btn-white:hover,.btn.btn-green:hover{
	color:#fff
}
.slider.slider-activities .overlay {
   background-color: {{settings.btn_bg_shop}} !important;
    opacity: .8;
}
.panel.panel-rich-text .with-list .txt {
    color: #000;
}
.panel.panel-rich-text .with-list .txt:hover {
    color: #2F8C59 !important;
}
.panel.panel-rich-text .with-bg .overlay{
	background:none
}
header .languages ul li a:hover,header .top .phone .txt:hover,header .h-right .nav-sp>ul>li>a:hover{
color:	{{settings.btn_bg_shop}} !important
}
.flexsocial img:hover {
    
    transform: scale(1.2);
    transition: 0.3s all ease;
}
.flexsocial img {

    transition: 0.3s all ease;
}

header .bottom .dropdown .isotope-filter a {
    color: {{settings.btn_bg_shop}}
   
}

